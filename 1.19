x= int(input("Enter the number of elements: "))


even_count = 0
odd_count = 0


print("Enter the numbers:")
for i in range(x):
    num = int(input())
    if num % 2 == 0:
        even_count += 1
    else:
        odd_count += 1


print(f"Number of even numbers: {even_count}")
print(f"Number of odd numbers: {odd_count}")
